{% extends 'base.html.twig' %}

{% block stylesheets %}
    <!-- Compiled and minified bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
{% endblock %}

{% block body %}
    <!-- Bootstrap navbar -->
    <nav class="navbar navbar-default">
        {% block bootstrap_navbar %}
        {% endblock %}
    </nav>
    <!-- Bootstrap container -->
    <div class="container bootstrap-container" id="main">
        {% block bootstrap_container %}
        {% endblock %}
    </div>
{% endblock %}

{%block javascripts %}
    <!-- jQuery library. It is required for bootstrap's js. -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    {# Only include jQuery UI if it was explicitly requested #}
    {% if include_jquery_ui|default %}
        <!-- jQuery UI library. Needed at least for the tooltips positioning -->
        <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/jquery-ui.min.js"></script>
    {% endif %}
    <!-- Compiled bootstrap JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
{% endblock %}
